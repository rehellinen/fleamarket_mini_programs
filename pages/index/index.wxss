/* theme */
.theme{
  display: flex;
  flex-direction: column;
  color: #6D6D6D;
}

.theme-text{
  text-align: center;
  margin-bottom: 5px;
  font-size: 35rpx;  
}

.theme-photo-container{
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  margin-bottom: 40rpx;
}

.theme-photo-container image{
  width: 48.5vw;
  height: 183rpx;
  margin-top: 10rpx;
  margin-left: 1vw;
}
