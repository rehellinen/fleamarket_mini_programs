.search-container{
  position: fixed;
  width: 80rpx;
  height: 80rpx;
  top: 30rpx;
  right: 20rpx;
  background-color: white; 
  border-radius:50px;
  opacity: 0.9; 
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 100;
}

.icon{
  width: 40rpx;
  height: 40rpx;
}

/* banner */
.banner{
  width: 750rpx;
  height: 415rpx;  
}

.banner image{
  width: 750rpx;
  height: 415rpx;  
}
